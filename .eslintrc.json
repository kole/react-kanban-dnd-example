{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "camelcase": 0,
    "comma-dangle": [2, "never"],
    "jsx-a11y/anchor-is-valid": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-underscore-dangle": [0, "always"],
    "no-unused-expressions": ["error", { "allowTaggedTemplates": true }]
  }
}
